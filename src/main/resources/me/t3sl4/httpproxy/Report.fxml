    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <TextField fx:id="ipTextField" layoutX="14.0" layoutY="87.0" prefHeight="25.0" prefWidth="170.0" promptText="Please enter the ip..." />
                <Button layoutX="212.0" layoutY="87.0" mnemonicParsing="false" onAction="#showReportButton" text="Show Report !" />
            </children></AnchorPane>
    </children>
</VBox>
